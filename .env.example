# KenPom Credentials
KENPOM_USERNAME=your_email@example.com
KENPOM_PASSWORD=your_password

# Oxylabs Proxy Configuration (for bypassing Cloudflare)
# Set PROXY_USE_SELENIUM_WIRE=true to enable proxy with authentication
PROXY_USE_SELENIUM_WIRE=false
OXY_USERNAME=your_oxylabs_username
OXY_PASSWORD=your_oxylabs_password
OXY_HOST=pr.oxylabs.io
OXY_PORT=7777

# Sticky Session (optional - maintains same IP throughout session)
# Format: any unique string, e.g., "sticky123" or "session1"
# Oxylabs will use: username-session-<OXY_STICKY>
OXY_STICKY=

# Turnstile Solver (optional - for Cloudflare challenges)
# If you use a Turnstile solving service like 2Captcha or Anti-Captcha:
# TURNSTILE_SOLVER_API_KEY=your_api_key_here

# Legacy proxy variables (deprecated - use new OXY_* variables above)
# PROXY_ENABLED=false
# PROXY_SERVER=pr.oxylabs.io:7777
# PROXY_USERNAME=your_proxy_username
# PROXY_PASSWORD=your_proxy_password
